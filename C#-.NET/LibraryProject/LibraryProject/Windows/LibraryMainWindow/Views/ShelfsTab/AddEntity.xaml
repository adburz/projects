<Window x:Class="LibraryProject.Windows.LibraryMainWindow.Views.Admin.ShelfsTab.AddEntity"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryProject.Windows.LibraryMainWindow.Views.Admin.ShelfsTab"
        mc:Ignorable="d"
        Title="AddEntity" Height="330" Width="600">
    <StackPanel>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Background="DarkGray" >
            <Button Content="Add book" Click="AddBook_Click"
                    Margin="8" FontSize="15" Width="100"/>
            <Button Content="Add magazine" Click="AddMagazine_Click" 
                    Margin="8" FontSize="15" Width="100"/>
        </StackPanel>
        <Frame x:Name="EntityType" NavigationUIVisibility="Hidden"/>


    </StackPanel>
</Window>
