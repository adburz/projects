<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.lab.controller.PatientsCountController">
   <children>
      <Button fx:id="SmithButton" layoutX="14.0" layoutY="83.0" mnemonicParsing="false" onAction="#SmithButton_click" text="Dr Smith" />
      <Button fx:id="JohnsonButton" layoutX="158.0" layoutY="83.0" mnemonicParsing="false" onAction="#JohnsonButton_click" text="Dr Johnson" />
      <Button fx:id="WilliamsButton" layoutX="303.0" layoutY="83.0" mnemonicParsing="false" onAction="#WilliamsButton_click" text="Dr Williams" />
      <Label layoutX="148.0" layoutY="57.0" text="Choose your doctor" />
      <Label layoutX="14.0" layoutY="368.0" text="Patients count:" />
      <Label fx:id="patientCountLabel" layoutX="111.0" layoutY="368.0" text="0" />
      <Button fx:id="toMainMenuButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#toMainMenuButton_click" text="To main menu" />
      <TableView fx:id="table" layoutX="15.0" layoutY="129.0" prefHeight="233.0" prefWidth="371.0">
        <columns>
          <TableColumn fx:id="Name" prefWidth="121.0" text="Name" />
          <TableColumn fx:id="LastName" prefWidth="127.0" text="Last name" />
            <TableColumn fx:id="PhoneNumber" prefWidth="122.0" text="Phone number" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
